<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="SiteUsageByGroup" tableDbType="NOT_IN_DB">
                <columns>
                    <column columnName="GroupName">
                        <isKeyField>true</isKeyField>
                    </column>
                    <column columnName="Visits">
                        <url>/query/executeQuery.view?schemaName=core&amp;
                            query.queryName=SiteUsagePerGroup&amp;
                            query.GroupName~eq=${GroupName}&amp;
                        </url>
                    </column>
                </columns>
                <titleColumn>GroupName</titleColumn>
            </table>
        </tables>
    </metadata>
</query>
