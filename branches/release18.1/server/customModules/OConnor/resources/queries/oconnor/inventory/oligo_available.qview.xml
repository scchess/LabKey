<customView xmlns="http://labkey.org/data/xml/queryCustomView">
<!--
Defines a custom view on the inventory table.
-->
    <columns>
        <column name="oligo_number"/>
        <column name="oligo_name"/>
        <column name="oligo_sequence"/>
        <column name="oligo_type"/>
        <column name="oligo_modifications"/>
        <column name="oligo_target"/>
        <column name="oligo_date"/>
        <column name="initials"/>
        <column name="comments"/>
        <column name="freezer"/>
        <column name="cane"/>
        <column name="box"/>
        <column name="box_row"/>
        <column name="box_column"/>
        <column name="coordinate"/>
    </columns>
    <filters>
        <filter column="sample_type" operator="eq" value="oligonucleotide"/>
    </filters>
    <sorts>
        <sort column="oligo_date" descending="true"/>
    </sorts>
</customView>

