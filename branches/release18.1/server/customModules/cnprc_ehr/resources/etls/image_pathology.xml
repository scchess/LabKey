<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Images - Pathology</name>
    <description>Images link to Pathology</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc_image" queryName="q_image_pathology" />
            <destination schemaName="cnprc_ehr" queryName="image_pathology" bulkLoad="true" batchSize="500" targetOption="merge">
                <alternateKeys>
                    <column name="objectid"/>
                </alternateKeys>
            </destination>
        </transform>
    </transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time">
        <deletedRowsSource schemaName="cnprcSrc_aud" queryName="q_image_pathology_delete" timestampColumnName="date_time" deletedSourceKeyColumnName="objectid" targetKeyColumnName="objectid"/>
    </incrementalFilter>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>