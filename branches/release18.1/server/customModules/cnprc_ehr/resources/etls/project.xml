<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Project</name>
<description>Project Lookup</description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="cnprcSrc" queryName="q_project"/>
        <destination schemaName="cnprc_ehr" queryName="project" bulkLoad="true" targetOption="merge">
            <alternateKeys>
                <column name="objectid"/>
            </alternateKeys>
        </destination>
    </transform>
</transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time">
        <deletedRowsSource schemaName="cnprcSrc_aud" queryName="q_project_delete" timestampColumnName="date_time" deletedSourceKeyColumnName="objectid" targetKeyColumnName="objectid"/>
    </incrementalFilter>
<schedule>
    <poll interval="60m" />
</schedule>
</etl>
