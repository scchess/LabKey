<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
<name>Cage Location History</name>
<description>Cage Location History - Additional columns </description>
<transforms>
    <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
        <description>Copy to target</description>
        <source schemaName="cnprcSrc" queryName="q_cage_location_history"/>
        <destination schemaName="cnprc_ehr" queryName="cage_location_history" bulkLoad="true" batchSize="5000" targetOption="merge">
            <alternateKeys>
                <column name="objectid"/>
            </alternateKeys>
        </destination>
    </transform>
</transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time">
        <deletedRowsSource schemaName="cnprcSrc_aud" queryName="q_cage_location_history_delete" timestampColumnName="date_time" deletedSourceKeyColumnName="objectid" targetKeyColumnName="objectid"/>
    </incrementalFilter>
<schedule>
    <!-- run at 1 am everyday -->
    <cron expression="0 0 1 * * ? *"/> <!--seconds, minutes, hours, day-of-month, month, day-of-week, year(optional) -->
</schedule>
</etl>
