<?xml version="1.0" encoding="UTF-8"?>
<etl xmlns="http://labkey.org/etl/xml">
    <name>Morning Health - Room</name>
    <description>Morning Health Room Observations</description>
    <transforms>
        <transform id="step1" type="org.labkey.di.pipeline.TransformTask">
            <description>Copy to target</description>
            <source schemaName="cnprcSrc" queryName="q_morningHealthObs_Rooms" />
            <destination schemaName="cnprc_ehr" queryName="cage_observations" bulkLoad="true" batchSize="5000" targetOption="merge">
                <columnTransforms>
                    <column source="MHO_BEGIN_DATE" target="date"/>
                </columnTransforms>
                <alternateKeys>
                    <column name="objectid"/>
                </alternateKeys>
            </destination>
        </transform>
    </transforms>
    <incrementalFilter className="ModifiedSinceFilterStrategy" timestampColumnName="date_time">
        <deletedRowsSource schemaName="cnprcSrc" queryName="q_morningHealthObs_Rooms_delete" timestampColumnName="date_time" deletedSourceKeyColumnName="objectid" targetKeyColumnName="objectid"/>
    </incrementalFilter>
    <schedule>
        <poll interval="60m" />
    </schedule>
</etl>