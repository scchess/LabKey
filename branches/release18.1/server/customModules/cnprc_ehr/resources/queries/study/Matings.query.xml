<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="matings" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="male">
                        <columnTitle>Sire</columnTitle>
                        <url>/ehr/participantView.view?participantId=${male}</url>
                    </column>
                    <column columnName="date">
                        <columnTitle>Mating Date</columnTitle>
                    </column>
                    <column columnName="hours">
                        <columnTitle>Hours</columnTitle>
                    </column>
                    <column columnName="cycleDay">
                        <columnTitle>Day of Cycle</columnTitle>
                    </column>
                    <column columnName="cycleStartDate">
                        <columnTitle>Cycle Start Date</columnTitle>
                    </column>
                    <column columnName="locationOverride">
                        <columnTitle>Location Override</columnTitle>
                    </column>
                    <column columnName="weekendBreeding">
                        <columnTitle>Weekend Breeding</columnTitle>
                    </column>
                    <column columnName="weekendBreedingState">
                        <columnTitle>Weekend Breeding State</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
