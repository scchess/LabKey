<customView xmlns="http://labkey.org/data/xml/queryCustomView">
    <columns>
        <column name="Id"/>
        <column name="gender/code">
            <properties>
                <property name="columnTitle" value="Sex"/>
            </properties>
        </column>
        <column name="Id/age/yearsAndMonths">
            <properties>
                <property name="columnTitle" value="Age today"/>
            </properties>
        </column>
        <column name="Id/curLocation/location">
            <properties>
                <property name="columnTitle" value="Location"/>
            </properties>
        </column>
        <column name="Id/MostRecentWeight/MostRecentWeight">
            <properties>
                <property name="columnTitle" value="Current Weight (kg)"/>
            </properties>
        </column>
        <column name="Id/DemographicsActiveColony/colonyCode"/>
        <column name="spf">
            <properties>
                <property name="columnTitle" value="SPF"/>
            </properties>
        </column>
        <column name="Id/DemographicsActiveAssignment/primaryProject"/>
        <column name="Id/DemographicsActivePayor/payor_ids"/>
        <column name="Id/DemographicsActiveAssignment/secondaryProjects"/>
        <column name="Id/flagList/values">
            <properties>
                <property name="columnTitle" value="Flags"/>
            </properties>
        </column>
        <column name="Id/Gestation/Gestation"/>
    </columns>
    <filters>
        <filter column="calculated_status" operator="eq" value="Alive"/>
        <filter column="Id/activeFlagList/values" operator="containsoneof" value="CH14;CH12;CH18;CH34;CH38;CH58;CH78;CHI1"/>
    </filters>
    <sorts>
        <sort column="Id" descending="false"/>
    </sorts>
</customView>