<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="WeightEncounter" tableDbType="NOT_IN_DB" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="date">
                        <columnTitle>Weight Date</columnTitle>
                    </column>
                    <column columnName="Weight">
                        <columnTitle>Weight</columnTitle>
                    </column>
                    <column columnName="TBWeight">
                        <columnTitle>TBWeight</columnTitle>
                    </column>
                    <column columnName="BirthWeight">
                        <columnTitle>BirthWeight</columnTitle>
                    </column>
                    <column columnName="TreatmentWeight">
                        <columnTitle>TreatmentWeight</columnTitle>
                    </column>
                    <column columnName="BehaviorAssessmentWeight">
                        <columnTitle>BehaviorAssessmentWeight</columnTitle>
                    </column>
                    <column columnName="Biopsy/NecropsyWeight">
                        <columnTitle>Biopsy/NecropsyWeight</columnTitle>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>
