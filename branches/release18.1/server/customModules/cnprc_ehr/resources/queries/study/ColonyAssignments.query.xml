<query xmlns="http://labkey.org/data/xml/query">
    <metadata>
        <tables xmlns="http://labkey.org/data/xml">
            <table tableName="colony_assignments" tableDbType="TABLE" useColumnOrder="true">
                <columns>
                    <column columnName="Id"/>
                    <column columnName="date">
                        <columnTitle>Assign Date</columnTitle>
                    </column>
                    <column columnName="enddate">
                        <columnTitle>Release Date</columnTitle>
                    </column>
                    <column columnName="colonyCode">
                        <columnTitle>Colony Code</columnTitle>
                        <fk>
                            <fkDbSchema>ehr_lookups</fkDbSchema>
                            <fkTable>colony_codes</fkTable>
                            <fkColumnName>value</fkColumnName>
                            <!--<fkDisplayColumnName>Title</fkDisplayColumnName>-->
                        </fk>
                    </column>
                </columns>
            </table>
        </tables>
    </metadata>
</query>