<tables xmlns="http://labkey.org/data/xml">
    <table tableName="AggregatedAnalysisResults" tableDbType="NOT_IN_DB">
        <columns>
            <column columnName="AnimalId">
                <columnTitle>Animal</columnTitle>
            </column>
            <column columnName="TotalReads">
                <columnTitle>Total Reads</columnTitle>
            </column>
            <column columnName="TotalIdentifiedReads">
                <columnTitle>Total Identified Reads</columnTitle>
            </column>
            <column columnName="TotalPercentUnknown">
                <columnTitle>Total % Unknown</columnTitle>
                <formatString>0.0'%'</formatString>
            </column>
            <column columnName="ConcatenatedHaplotypes">
                <columnTitle>Concatenated Haplotypes</columnTitle>
            </column>
            <column columnName="InconsistentAssignments">
                <conditionalFormats>
                    <conditionalFormat>
                        <filters>
                            <filter value="true" operator="eq"/>
                        </filters>
                        <backgroundColor>FF8080</backgroundColor>
                    </conditionalFormat>
                </conditionalFormats>
            </column>
        </columns>
    </table>
</tables>
