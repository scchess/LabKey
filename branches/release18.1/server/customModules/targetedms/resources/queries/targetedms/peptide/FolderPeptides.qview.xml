<customView xmlns="http://labkey.org/data/xml/queryCustomView" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        label="All Folder Peptides">
    <columns>
        <column name="PeptideGroupId/RunId/File" />
        <column name="PeptideGroupId/Label" />
        <column name="ModifiedPeptideDisplayColumn" /> <!-- Defined in ModifiedSequenceDisplayColumn.java -->
        <column name="StartIndex" />
        <column name="EndIndex" />
        <column name="CalcNeutralMass" />
        <column name="NumMissedCleavages" />
        <column name="RtCalculatorScore" />
        <column name="PredictedRetentionTime" />
        <column name="AvgMeasuredRetentionTime" />
        <column name="NoteAnnotations" />
        <column name="RepresentivePrecursorCount" />
    </columns>
    <sorts>
       <sort column="PeptideGroupId/Label" />
       <sort column="Sequence" />
       <sort column="PeptideGroupId/RunId/File" />
    </sorts>
</customView>