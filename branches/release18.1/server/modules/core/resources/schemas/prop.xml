<tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\schemas\tableInfo.xsd" xmlns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<table tableName="Properties" tableDbType="TABLE">
        <cacheSize>10000</cacheSize>
		<columns>
			<column columnName="Set"/>
			<column columnName="Name"/>
			<column columnName="Value"/>
		</columns>
	</table>
	<table tableName="PropertySets" tableDbType="TABLE">
		<columns>
			<column columnName="Set"/>
			<column columnName="ObjectId"/>
			<column columnName="Category"/>
			<column columnName="UserId"/>
			<column columnName="Encryption"/>
		</columns>
	</table>
	<table tableName="PropertyEntries" tableDbType="VIEW">
		<columns>
			<column columnName="ObjectId"/>
			<column columnName="Category"/>
			<column columnName="UserId"/>
			<column columnName="Name"/>
			<column columnName="Value"/>
		</columns>
	</table>
</tables>
