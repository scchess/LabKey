 <tables xmlns="http://labkey.org/data/xml">

  <table tableName="statistic" tableDbType="TABLE">
    <columns>
      <column columnName="objectid"/>
      <column columnName="statisticid"/>
      <column columnName="value"/>
      <column columnName="originalstatisticid"/>
    </columns>
  </table>
  <table tableName="statisticattr" tableDbType="TABLE">
    <columns>
      <column columnName="rowid"/>
      <column columnName="container"/>
      <column columnName="name"/>
      <column columnName="id"/>
    </columns>
  </table>

  <table tableName="graph" tableDbType="TABLE">
    <columns>
      <column columnName="objectid"/>
      <column columnName="graphid"/>
      <column columnName="data"/>
      <column columnName="originalgraphid"/>
    </columns>
  </table>
  <table tableName="graphattr" tableDbType="TABLE">
    <columns>
      <column columnName="rowid"/>
      <column columnName="container"/>
      <column columnName="name"/>
      <column columnName="id"/>
    </columns>
  </table>

  <table tableName="keyword" tableDbType="TABLE">
    <columns>
      <column columnName="objectid"/>
      <column columnName="keywordid"/>
      <column columnName="value"/>
      <column columnName="originalkeywordid"/>
    </columns>
  </table>
  <table tableName="keywordattr" tableDbType="TABLE">
    <columns>
      <column columnName="rowid"/>
      <column columnName="container"/>
      <column columnName="name"/>
      <column columnName="id"/>
    </columns>
  </table>

  <table tableName="object" tableDbType="TABLE">
    <columns>
      <column columnName="rowid"/>
      <column columnName="container"/>
      <column columnName="dataid"/>
      <column columnName="typeid"/>
      <column columnName="uri"/>
      <column columnName="compid"/>
      <column columnName="scriptid"/>
      <column columnName="fcsid"/>
    </columns>
  </table>

  <table tableName="script" tableDbType="TABLE">
    <columns>
      <column columnName="rowid"/>
      <column columnName="objectid"/>
      <column columnName="text"/>
    </columns>
  </table>

</tables>
