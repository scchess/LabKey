<tables xsi:schemaLocation="http://labkey.org/data/xml ..\..\..\..\..\schemas\tableInfo.xsd"
        xmlns="http://labkey.org/data/xml" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <table tableName="TransformRun" tableDbType="TABLE">
        <columns>
            <column columnName="TransformRunId"/>
            <column columnName="RecordCount"/>
            <column columnName="Container"/>
            <column columnName="JobId"/>
            <column columnName="TransformId"/>
            <column columnName="TransformVersion"/>
            <column columnName="Status"/>
            <column columnName="StartTime"/>
            <column columnName="EndTime"/>
            <column columnName="TransformRunLog"/>
            <column columnName="Created"/>
            <column columnName="CreatedBy"/>
            <column columnName="Modified"/>
            <column columnName="ModifiedBy"/>
        </columns>
    </table>
    <table tableName="TransformConfiguration" tableDbType="TABLE">
        <columns>
            <column columnName="RowId"/>
            <column columnName="Container"/>
            <column columnName="TransformId"/>
            <column columnName="Enabled"/>
            <column columnName="VerboseLogging"/>
            <column columnName="LastChecked"/>
            <column columnName="TransformState"/>
            <column columnName="Created"/>
            <column columnName="CreatedBy"/>
            <column columnName="Modified"/>
            <column columnName="ModifiedBy"/>
        </columns>
    </table>
</tables>
